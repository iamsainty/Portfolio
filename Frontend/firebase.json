{
  "hosting": {
    "public": "build",
    "redirects": [
      {
        "source": "http://www.hey-sainty.web.app",
        "destination": "https://hey-sainty.web.app",
        "type": 301
      },
      {
        "source": "https://www.hey-sainty.web.app",
        "destination": "https://hey-sainty.web.app",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/sitemap.xml",
        "headers": [
          { "key": "Content-Type", "value": "application/xml" },
          { "key": "Cache-Control", "value": "public, max-age=86400" }
        ]
      }
    ]
  }
}